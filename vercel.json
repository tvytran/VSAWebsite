{
  "version": 2,
  "builds": [
    {
      "src": "vsa-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://vsa-website.vercel.app/api/:path*"
    }
  ]
}